// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:root@localhost:3306/shopify"
}

model ShopSession {
  id      String @id
  shop    String? 
  payload Json
  
}

model User {
  id Int     @id @default(autoincrement())
  account_number   String
  user_name   String  
  password	  String
  company     String?
  country     String?
  city        String?
  email       String?
  address1    String?
  address2    String?
  contact     String?
  mobile      String?
  shop_url    String? @unique  
}
